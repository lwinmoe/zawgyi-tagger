{
  "name": "Zawgyi Font Tagger",
  "description": "Tool to tag and render Zawgyi font in Burmese.",
  "version": "1.0.2",
  "permissions": ["contextMenus", "tabs", "http://*/*", "https://*/*"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery.min.js", "mm.font.tools.js"]
    }
  ],
  "web_accessible_resources": [
    "mm.css",
    "ZawgyiOne.ttf"
  ],  
  "manifest_version": 2
}
