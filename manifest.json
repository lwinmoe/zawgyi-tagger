{
  "manifest_version": 2,
  "name": "Zawgyi Font Tagger",
  "version": "1.0.3",
  "description": "Tool to tag and render Zawgyi font in Burmese.",
  "permissions": ["tabs", "http://*/*", "https://*/*"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["zg.js"]
    }
  ],
  "web_accessible_resources": [
    "zg.css",
    "ZawgyiOne.ttf"
  ]
}
